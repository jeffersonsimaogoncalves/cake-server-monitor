{
  "name": "jeffersonsimaogoncalves/cakephp-server-monitor",
  "description": "Server Monitor built with CakePHP 3",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=7.0",
    "cakephp/cakephp": "^3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7|^6.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Li",
      "email": "welcome@watchowl.com"
    },
    {
      "name": "Jefferson Simão Gonçalves",
      "homepage": "https://github.com/jeffersonsimaogoncalves"
    }
  ],
  "autoload": {
    "psr-4": {
      "JeffersonSimaoGoncalves\\CakeServerMonitor\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JeffersonSimaoGoncalves\\CakeServerMonitor\\Test\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "htaccess-protect": true,
    "optimize-autoloader": true
  }
}
