{
    "name": "watchowl/cake-server-monitor",
    "description": "Server Monitor built with CakePHP 3",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.0",
        "cakephp/cakephp": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Li",
            "email": "welcome@watchowl.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "WatchOwl\\CakeServerMonitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WatchOwl\\CakeServerMonitor\\Test\\": "tests"
        }
    }
}
